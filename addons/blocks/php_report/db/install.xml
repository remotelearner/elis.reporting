<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/php_report/db" VERSION="20110330" COMMENT="XMLDB file for Moodle blocks/php_report"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="php_report_schedule" COMMENT="Scheduled reports">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="report owner" PREVIOUS="id" NEXT="report"/>
        <FIELD NAME="report" TYPE="char" LENGTH="63" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="The report type" PREVIOUS="userid" NEXT="config"/>
        <FIELD NAME="config" TYPE="text" LENGTH="medium" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="Serialized configuration for the scheduled task" PREVIOUS="report"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="userid_idx" UNIQUE="false" FIELDS="userid" NEXT="report_idx"/>
        <INDEX NAME="report_idx" UNIQUE="false" FIELDS="report" PREVIOUS="userid_idx"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>